{
  "name": "app",
  "version": "1.0.50",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/server.ts",
    "prettier:check": "bunx prettier --check src/**/*.ts",
    "prettier:write": "bunx prettier --write src/**/*.ts",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "bun run lint --fix",
    "test:coverage": "bunx vitest run --coverage",
    "test:unit": "bunx vitest run src/test --exclude src/test/integration",
    "test:unit:coverage": "bunx vitest run --coverage --exclude src/test/integration",
    "test:integration": "bunx vitest run src/test/integration",
    "test:integration:coverage": "bunx vitest run --coverage src/test/integration",
    "prisma:generate": "bun run prisma generate",
    "prisma:migrate": "bun run prisma migrate deploy",
    "seed": "bun run prisma/database-seed.ts",
    "reset:development": "curl -i -X POST http://localhost:3004/api/v1/reset/ -H 'Content-type: application/json'",
    "reset:development:local": "curl -i -X POST http://localhost:3000/api/v1/reset/ -H 'Content-type: application/json'",
    "reset:test": "curl -i -X POST http://localhost:3001/api/v1/reset/ -H 'Content-type: application/json'",
    "reset:test:local": "curl -i -X POST http://localhost:3000/api/v1/reset/ -H 'Content-type: application/json'",
    "build:development:local": "bun run build:bundle && bun run build:copy:development:local",
    "build:bundle": "bun build src/server.ts --minify-whitespace --minify-syntax --sourcemap=inline --target bun --outdir dist/server",
    "build:development": "bun run build:bundle && bun run build:copy:development",
    "build:copy:development": "cp .env.development dist/.env.development && cp -R prisma dist/prisma",
    "build:copy:development:local": "cp .env.development.local dist/.env.development.local && cp -R prisma dist/prisma",
    "build:stage": "bun run build:bundle && bun run build:copy:stage",
    "build:copy:stage": "cp .env.test dist/.env.test && cp -R prisma dist/prisma",
    "build:stage:local": "bun run build:bundle && bun run build:copy:stage:local",
    "build:copy:stage:local": "cp .env.test.local dist/.env.test.local && cp -R prisma dist/prisma",
    "build:stage:node": "npm run build:bundle:node && npm run build:copy:stage:node",
    "build:bundle:node": "tsup src/server.ts --format cjs --target node20 --out-dir dist --minify --sourcemap",
    "build:copy:stage:node": "cpx .env.test dist/.env.test && cpx 'prisma/**/*' dist/prisma",
    "sonar": "sonar-scanner",
    "stage": "bun --env-file=.env.test dist/server/server.js && echo 'Server started: [TEST-STAGE]'",
    "stage:node": "node dist/server.js && echo 'Server started: [TEST-STAGE-NODE-IMAGE]'",
    "prod": "bun --env-file=.env.prod bun run dist/server.js",
    "docker:build:stage": "docker build --build-arg BUN_ENV=stage -t dartw/app:stage .",
    "docker:push:stage": "docker push dartw/app:stage"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/opentelemetry": "^1.3.1",
    "@elysiajs/swagger": "^1.3.1",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.203.0",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@prisma/client": "^6.12.0",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.1",
    "elysia": "latest",
    "ioredis": "^5.7.0",
    "pg": "^8.15.6",
    "prisma": "^6.12.0",
    "redis": "^5.8.2",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@opentelemetry/instrumentation-pg": "^0.55.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "bun-types": "latest",
    "c8": "^10.1.3",
    "cpx": "^1.5.0",
    "eslint": "^9.32.0",
    "monocart-coverage-reports": "^2.12.9",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "module": "src/server.js"
}
